{
    "name": "dnadesign/dna-recipe-theme-default",
    "description": "DNA's default theme recipe",
    "type": "silverstripe-recipe",
    "keywords": ["silverstripe", "dna"],
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "^1",
        "stevie-mayhew/silverstripe-svg": "^2.1",
        "benmanu/silverstripe-styleguide": "dev-feature/vendor as 0.1"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
        "project-files": [
            "mysite/_config/*",
            "mysite/css/*",
            "mysite/styleguide/*",
            "theme-default/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "license": "BSD-3-Clause",
    "prefer-stable": true,
    "minimum-stability": "dev"
}
